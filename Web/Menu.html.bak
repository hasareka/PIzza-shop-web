<style>
*{box-sizing: border-box}
body {font-family: Arial, Helvetica, sans-serif;}

header {
  
  background-image:url(../Images/order.png);
  background-repeat:no-repeat;
  background-size:cover;
  height:200px;
  padding: 30px;
  text-align: center;
  font-size: 60px;
  color: white;
}

.navbar {
  width: 100%;
  background-color:#2F3036;
  overflow: auto;
}

.navbar a {
  float: left;
  padding: 12px;
  color: white;
  text-decoration: none;
  font-size: 20px;
  width: 25%; 
  text-align: center;
}

.navbar a:hover {
  background-color: #65BFB2;
}

.navbar a.active {
  background-color: #84C4E5;
}
.pizza{
	width: 100%;
	height: 165vh;
			
}
.appetizer{
	width: 100%;
			height: 105vh;
			
}
.desserts{
	width: 100%;
			height: 60vh;
			
}
.pasta{
	width: 100%;
			height: 100vh;
			
}
 table,td,{
	border:1px white;
}
table{
	border-collapse:collapse;
	width:100%;
	
}
td {
  padding:15px;	
  text-align: left;
  
}
span{
	font-size:15px;
	color:red;
	text-align:right;
	font-style:italic;
}
.appetizer h1,.desserts h1, .pasta h1{
	background-color:#65BFB2;
}



@media screen and (max-width: 500px) {
  .navbar a {
    float: none;
    display: block;
    width: 100%;
    text-align: left;
  }
}
</style>
